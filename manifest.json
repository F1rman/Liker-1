{
  "name": "Liker â€“ Instagram bot",
  "description": "Bring attention to your profile for free",
  "version": "2",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "src/bg/headers.js",
      "src/bg/utils.js",
      "src/bg/main.js",
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon512.png",
    "default_title": "browser action demo",
    "default_popup": "src/popup/popup.html"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "alarms",
    "background",
    "power",
    "storage",
    "webRequest",
    "unlimitedStorage",
    "webRequestBlocking",
    "*://*.instagram.com/*"
  ]
}
